body {
    width: 50%;
    height: 600px;
    margin: auto;
    min-width: 800px;
}

.header {
    margin: auto;
    width: 770px;
    text-align: center;
    padding: 5px;
    position: relative;
}

.logo {
    margin: auto;
}

span {
    font-family: 'Raleway', sans-serif;
    font-size: 50px;
}

#main {
    border-radius: 5px;
    border: 2px dashed #008000;
    width: 580px;
    height: 280px;
    margin: 25px auto 0px;
}